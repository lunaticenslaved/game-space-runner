{
  "name": "space-runner",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon app/server/index.ts",
    "build:server": "tsc -p ./app/server/tsconfig.json",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --write",
    "preview": "vite preview",
    "db:migrate": "npx prisma migrate dev --schema app/server/prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema app/server/prisma/schema.prisma"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^9.0.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.409.0",
    "@prisma/client": "^5.2.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.2.0",
    "sass": "^1.66.1",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^3.2.0"
  }
